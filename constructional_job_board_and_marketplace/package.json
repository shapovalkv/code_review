{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "ssr-dev": "mix --mix-config=webpack.inertia.mix.js && mix --production --mix-config=webpack.ssr.mix.js",
        "ssr-prod": "mix --production --mix-config=webpack.inertia.mix.js && mix --production --mix-config=webpack.ssr.mix.js",
        "ssr-watch": "mix --mix-config=webpack.inertia.mix.js watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production && npm run f-prod",
        "production": "mix --production",
        "f-dev": "cross-env process.env.section=frontend mix",
        "f-watch": "npm run f-dev watch",
        "f-prod": "cross-env process.env.section=frontend mix --production",
        "all-dev": "npm run dev && npm run f-dev",
        "all-watch": "npm run watch && npm run f-watch"
    },
    "devDependencies": {
        "@vue/compiler-sfc": "^3.2.37",
        "cross-env": "^5.2.1",
        "jquery": "^3.6.0",
        "laravel-mix": "^6.0.49",
        "resolve-url-loader": "4.0.0",
        "sass": "^1.32.12",
        "sass-loader": "12.1.0",
        "sass-resources-loader": "^2.2.5",
        "vue-loader": "^17.0.0",
        "webpack-node-externals": "^3.0.0"
    },
    "dependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@fawmi/vue-google-maps": "^0.9.72",
        "@inertiajs/inertia": "^0.11.0",
        "@inertiajs/inertia-vue3": "^0.6.0",
        "@inertiajs/progress": "^0.2.7",
        "@inertiajs/server": "^0.1.0",
        "@picmo/popup-picker": "^5.7.0",
        "@tinymce/tinymce-vue": "^4.0.7",
        "@vue/server-renderer": "^3.2.27",
        "@vueform/slider": "^2.1.2",
        "@vuepic/vue-datepicker": "^3.4.8",
        "autosize": "^5.0.1",
        "axios": "^0.27.2",
        "bootbox": "^5.5.2",
        "bootstrap": "^5.2.0",
        "bootstrap-vue-3": "^0.1.21",
        "dayjs": "^1.11.4",
        "eslint": "^8.12.0",
        "fecha": "^4.2.3",
        "font-awesome": "^4.7.0",
        "he-tree-vue": "^3.0.3",
        "ionicons": "^4.6.3",
        "lodash": "^4.17.21",
        "maska": "^1.5.0",
        "mitt": "^3.0.0",
        "picmo": "^5.7.0",
        "popper.js": "^1.16.1",
        "remixicon": "^2.5.0",
        "swiper": "^8.3.2",
        "v-money3": "^3.21.0",
        "vue": "^3.2.36",
        "vue-form-generator": "^2.3.4",
        "vue-select": "^4.0.0-beta.3",
        "vue-social-sharing": "^4.0.0-alpha4",
        "vue3-cookies": "^1.0.6",
        "vue3-popper": "^1.5.0",
        "vuedraggable": "4.1.0"
    }
}
